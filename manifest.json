{
    "name": "DisChat",
    "version": "1.1",
    "description": "A chrome extention that uses webservers to chat with your friends",
    "browser_action":{
        "default_popup" : "popup.html"
    },
    "icons":{
        "16":"icon16.png",
        "48":"icon48.png",
        "128":"icon128.png"
    },
    "permissions":[
        "*://*:*/",
        "storage"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "manifest_version": 2
  }
